<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hyerim.my.foodstreet">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_meal_foreground"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_meal_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainDetailActivity"></activity>
        <activity android:name=".auth.LoginActivity" />
        <activity android:name=".auth.JoinActivity" />
        <activity android:name=".FoodListActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.naver.maps.map.CLIENT_ID"
            android:value="sad7m2l0u2" />
    </application>

</manifest>